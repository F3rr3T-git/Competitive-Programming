#include <bits/stdc++.h>
using namespace std;
 
#define endl '\n'
 
int main()
{
        int i;
        string s;
        cin >> s;
        set<char> st;
        
        for(i=0;i<s.length();i++)
        st.insert(s[i]);
        
        if(st.size()%2==0)
        cout << "CHAT WITH HER!" << endl;
        else
        cout << "IGNORE HIM!" << endl;
        
}
