#include <iostream>
#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
      int i,a[26],x=0,y=0;
      
      for(i=1;i<26;i++)
      cin >> a[i];
      
      for(i=1;i<26;i++)
      {
            if(a[i]==1)
            {
                   if(i%5==0)
                   x =  (i/5) ;
                   else
                   x = (i/5) +1 ;
                   
                   if(i%5==0)
                   y =  5 ;
                   else
                   y = i%5;
            }
            
      }
        
      cout << abs(3-x)+abs(3-y) << endl;
      
      return 0;
}
