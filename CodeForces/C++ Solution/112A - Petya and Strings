#include <bits/stdc++.h> 
using namespace std;
 
 
 
int main()
{
     int i, c=0;
     string s,t;
     cin >> s >> t;
     
     for(i=0;i<s.length();i++)
     {
        s[i] = tolower (s[i]);
        t[i] = tolower (t[i]);
     }
     
     for(i=0;i<s.length();i++)
     {
           if(int(s[i])>int(t[i]))
           {
                 c = 1;
                 break;
           }
           else if(int(s[i])<int(t[i]))
           {
                 c= -1;
                 break;
           }
     }
     
     
     cout << c << endl;
    
      return 0;
}
