#include <bits/stdc++.h>
using namespace std;
 
int main()
{
         int i,u=0,l=0;
         string s;
         cin >> s;
         
         for(i=0;i<s.length();i++)
         {
             if(isupper(s[i]))
             u++;
             else
             l++;
         }
         
         if(u>l)
         {
                  for(i=0;i<s.length();i++)
                  putchar(toupper(s[i]));
         }
         else if(u<=l)
         {
                  for(i=0;i<s.length();i++)
                  putchar(tolower(s[i]));
         }
         
         
         return 0;
}
