#include <bits/stdc++.h>
using namespace std;
 
int main()
{
        int i,n;
        string s;
        cin >> n >> s;
        set<char> st;
        
        for(i=0;i<n;i++)
        st.insert(tolower(s[i]));
        
        if(st.size()==26)
        cout << "YES" << endl;
        else
        cout << "NO" << endl;
        
        
}
