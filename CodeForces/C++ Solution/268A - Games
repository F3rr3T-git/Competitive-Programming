#include <iostream>
#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
      int n,i,hr,ar,sm=0;
      cin >> n;
      int h[101]={0},a[101]={0};
      
      for(i=0;i<n;i++)
      {
            cin >> hr >> ar;
            h[hr]++;
            a[ar]++;
      }
      
     
      for(i=1;i<101;i++)
      {
            sm += h[i]*a[i];
 
      }
     
     cout << sm << endl;
     
      return 0;
}
