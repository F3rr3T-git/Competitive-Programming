#include <bits/stdc++.h>
using namespace std;
 
int main()
{
     int n;
     string s;
     cin >> n;
     
     n++;
    
     while(1)
     {
          s = to_string(n);
          if(s[0]!=s[1] && s[0]!=s[2] && s[0]!=s[3] && s[1]!=s[2] && s[1]!=s[3] && s[2]!=s[3])
          {break;}
          
          n++;
     }
     
     cout << n << endl;
     
     return 0;
     
}
