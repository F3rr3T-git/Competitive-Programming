#include <bits/stdc++.h>
using namespace std;
 
int main()
{
         
         int i,n;
         string s;
         cin >> n >> s;
         map<char,int> mp;
         
         for(i=0;i<n;i++)
         mp[s[i]]++;
         
         if(mp['A']>mp['D'])
         cout << "Anton" << endl;
         else if(mp['A']<mp['D'])
         cout << "Danik" << endl;
         else
         cout << "Friendship" << endl;
}
