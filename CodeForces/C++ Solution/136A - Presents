#include <iostream>
#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
      int i,n;
      cin >> n;
      int ar[n+1],br[n+1];
      
      for(i=1;i<=n;i++)
      {
        cin >> ar[i];   
      }
      
      for(i=1;i<=n;i++)
      {
            br[ar[i]] = i;
      }
      
      for(i=1;i<=n;i++)
       cout << br[i] << " ";
      
      return 0;
}
