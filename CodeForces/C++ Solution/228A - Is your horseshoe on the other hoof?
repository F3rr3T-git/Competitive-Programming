#include <bits/stdc++.h>
using namespace std;
 
int main()
{
        long long int a,b,c,d;
        cin >> a >> b >> c >> d;
        set<long long int> st;
        
        st.insert(a);
        st.insert(b);
        st.insert(c);
        st.insert(d);
        
        if(st.size()==1)
        cout << 3 << endl;
        else if(st.size()==2)
        cout << 2 << endl;
        else if(st.size()==3)
        cout << 1 << endl;
        else if(st.size()==4)
        cout << 0 << endl;
        
        return 0;
}
