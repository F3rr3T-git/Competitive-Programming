#include <bits/stdc++.h>
using namespace std;
 
int main()
{
         int n,m;
         cin >> n >> m;
         
         if(n%2==0 && m%2==0)
         cout << (n/2)*m << endl;
         else if(n%2!=0 && m%2==0)
         cout << (m/2)*n << endl;
         else if(n%2==0 && m%2!=0)
         cout << (n/2)*m <<endl;
         else
         cout << ((n-1)/2)*(m-1) + n/2 + m/2 << endl;
         
         return 0;
}
