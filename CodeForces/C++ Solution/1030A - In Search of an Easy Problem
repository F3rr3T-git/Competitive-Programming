#include <bits/stdc++.h>
using namespace std;
 
int main()
{
        int n,i,c=0,r;
        cin >> n;
        
        for(i=0;i<n;i++)
        {
                cin >> r;
                c += r;
        }
        
        if(c!=0)
        cout << "HARD" << endl;
        else
        cout << "EASY" << endl;
        
        return 0;
}
