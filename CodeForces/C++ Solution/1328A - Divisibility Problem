#include <bits/stdc++.h>
using namespace std;
 
int main()
{
     int t;
     cin >> t;
     
     while(t--)
     {
          long long int a,b,r;
          cin >> a >> b;
          
          if(a%b != 0)
          {
               r = a/b;
               r++;
               r *= b;
               a = r-a;
               cout << a << endl;
          }
          else 
          cout << 0 << endl;
          
          
          
     }
     
     
     return 0;
}
