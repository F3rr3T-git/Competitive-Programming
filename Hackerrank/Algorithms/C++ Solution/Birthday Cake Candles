#include <bits/stdc++.h>
using namespace std;

int main()
{
    int i;
    long long int n,c=0;
    cin >>n;
    long long int a[n];

    for(i=0;i<n;i++)
    {
        cin >> a[i];
    }

     sort(a, a+n); 
    
    i = n-1;

    while(a[i]==a[n-1])
    {
        c++;
        i--;
    }

    cout << c << endl;

}
