#include <bits/stdc++.h>
using namespace std;

int main ()
{
    int i,d,c=0;
    long long int n,m=0;
    cin >> n;
    long long int a[6]={0};

    for(i=0;i<n;i++)
    {
        cin >> d;
        a[d]++;
    }

    for(i=1;i<=5;i++)
    {
        if(a[i]>m)
       { 
           m = a[i];
           c = i;
       }
    }

    cout << c << endl;


return 0;
}
