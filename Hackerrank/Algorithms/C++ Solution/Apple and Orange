#include <bits/stdc++.h>
using namespace std;

int main()
{
    int i;
    long int s,t,a,b,m,n,d,A=0,B=0;

    cin >> s >> t;
    cin >> a >> b;
    cin >> m >> n;

    for(i=0;i<m;i++)
    {
        cin >> d;
        d = a+d;

        if(d>=s && d<=t)
        A++;
    }
    
    
    for(i=0;i<n;i++)
    {
        cin >> d;
        d = b + d ;

        if(d>=s && d<=t)
        B++;
    }

     
     cout << A << endl << B << endl;

    return 0;
}
