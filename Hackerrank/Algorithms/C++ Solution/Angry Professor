#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;

    while(t--)
    {
        int i,n,k,r,c=0;
        cin >> n  >> k;

        for(i=0;i<n;i++)
        {
            cin >> r;
            if(r<=0)
            c++;
        }

        if(c < k)
        cout << "YES" << endl;
        else
        cout << "NO" << endl;

    }
return 0;
}
