#include <bits/stdc++.h>
using namespace std;

int main ()
{
    int i,a[3],b[3],A=0,B=0;
     
     for(i=0;i<3;i++)
       cin >> a[i];

     for(i=0;i<3;i++)
       cin >> b[i];
     
     
     for(i=0;i<3;i++)
     {
         if(a[i]>b[i])
         A++;
         else if(a[i]<b[i])
         B++;
         else if(a[i]==b[i])
         ;
         else; 
     }

     cout <<  A << " " << B << endl;
      
    return 0;
}
